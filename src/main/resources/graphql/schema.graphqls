type User {
    id: ID
    firstName: String
    lastName: String
    age: Int
    phoneNumber: String
    email: String
}

input UserInput {
    firstName: String!
    lastName: String
    age: Int
    phoneNumber: String
    email: String!
}

type Query {
    getUser(id: ID!): User
}

type Mutation {
    createUser(user: UserInput!): User
}
